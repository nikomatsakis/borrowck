// Corresponds to:
//
// ```
// let foo = Vec { dummy: () };
// let p = &foo;
// mem::drop(foo.dummy);
// ```

struct Vec<+> {
  field: 0
}  

let foo: Vec<()>;
let p: &'_ Vec<()>;

block START {
    foo = use();
    p = &'_ foo;
    drop(foo.field); //! ERROR `foo` is borrowed
    use(p);
    StorageDead(foo);
}

